# Configuración del frontend
VITE_API_URL=http://localhost:8000
VITE_APP_NAME="Sistema de Gestión Excel - Laboratorio"
VITE_APP_VERSION="1.0.0"
VITE_ENVIRONMENT=development

# Configuración de la aplicación
VITE_MAX_FILE_SIZE=10485760  # 10MB
VITE_ALLOWED_FILE_TYPES=xlsx,xls,pdf

# Configuración de logging
VITE_LOG_LEVEL=info

# Configuración de PWA
VITE_PWA_ENABLED=true
VITE_PWA_NAME="LabExcel"
VITE_PWA_SHORT_NAME="LabExcel"
VITE_PWA_DESCRIPTION="Sistema de Gestión Excel para Laboratorio"

# Configuración de desarrollo
VITE_DEV_SERVER_PORT=3001
VITE_DEV_SERVER_HOST=localhost
